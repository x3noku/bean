{
    "$schema": "https://turbo.build/schema.json",
    "ui": "tui",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
            "outputs": ["dist/**"]
        },
        "dev": {
			"dependsOn": ["^dev"],
            "cache": false,
            "persistent": true
        },
        "check": {
			"dependsOn": ["^check"]
        },
        "check:fix": {
			"dependsOn": ["^check:fix"],
            "cache": false
        },
        "ci": {
            "dependsOn": ["^ci"]
        },
        "typecheck": {
            "dependsOn": ["^typecheck"]
        },
        "clean": {
            "cache": false
        },
        "//#clean": {
            "cache": false
        },
        "db:push": {
            "cache": false,
            "persistent": true
        },
        "db:studio": {
            "cache": false,
            "persistent": true
        },
        "db:migrate": {
            "cache": false,
            "persistent": true
        },
        "db:generate": {
            "cache": false,
            "persistent": true
        },
        "ui-add": {
            "cache": false,
            "interactive": true
        }
    }
}
