{
    "$schema": "https://turborepo.com/schema.json",
    "ui": "tui",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"],
            "outputs": [".cache/tsbuildinfo.json", "dist/**"]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "check": {
            "dependsOn": ["^check"]
        },
        "check:fix": {
            "dependsOn": ["^check:fix"],
            "cache": false
        },
        "ci": {
            "dependsOn": ["^ci"]
        },
        "typecheck": {
            "dependsOn": ["^build"],
            "outputs": [".cache/tsbuildinfo.json"]
        },
        "clean": {
            "cache": false
        },
        "//#clean": {
            "cache": false
        },
        "db:studio": {
            "cache": false,
            "persistent": true
        },
        "db:push": {
            "cache": false,
            "persistent": true
        },
        "db:migrate": {
            "cache": false,
            "persistent": true
        },
        "db:generate": {
            "cache": false,
            "persistent": true
        }
    }
}
